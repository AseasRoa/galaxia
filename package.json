{
  "name": "galaxia",
  "version": "0.0.1",
  "author": {
    "name": "AseasRoa"
  },
  "description": "Full-Stack Framework",
  "license": "MIT",
  "type": "module",
  "types": "./types/index.d.ts",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./fileSystem": "./lib/exports/fileSystem.js",
    "./docschema": "./lib/exports/docschema.js",
    "./paintor": "./lib/exports/paintor.js",
    "./db/mongo": "./lib/exports/db/mongo.js"
  },
  "imports": {
    "#galaxia": {
      "default": "./lib/index.js"
    },
    "#galaxia/db/mongo": {
      "default": "./lib/exports/db/mongo.js"
    }
  },
  "scripts": {
    "check": "npm run check:tsc & npm run check:eslint",
    "check:eslint": "eslint --config eslint.config.js ./",
    "check:tsc": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "autocannon": "autocannon --connections 15 --duration 10 http://localhost:8000/helloWorld"
  },
  "dependencies": {
    "chart.js": "^4.4.7",
    "docschema": "^1.3.1",
    "ejs": "^3.1.10",
    "esbuild": "^0.24.2",
    "handlebars": "^4.7.8",
    "http2-proxy": "^5.0.53",
    "json5": "^2.2.3",
    "marked": "^15.0.6",
    "mongodb": "^6.12.0",
    "paintor": "^1.4.4",
    "pug": "^3.0.3",
    "sass": "^1.83.1",
    "strip-comments": "^2.0.1",
    "stylus": "^0.64.0",
    "watcher": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/pug": "^2.0.10",
    "@types/stylus": "^0.48.43",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/eslint-plugin": "^1.1.24",
    "autocannon": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "jsdom": "^26.0.0",
    "lightningcss": "^1.29.1",
    "mongodb-memory-server": "^10.1.3",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AseasRoa/galaxia.git"
  },
  "homepage": "https://github.com/AseasRoa/galaxia",
  "bin": {
    "galaxia": "bin/galaxia.js"
  },
  "engines" : {
    "node" : ">=18.0.0"
  }
}
